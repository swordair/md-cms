{{!< default}}

<div class="container-fluid">
    <div class="row">

        
        
        <div class="main">
            <h1 class="page-header">Page list</h1>
            <div class="">
                {{#each pageGroup}}
                <h3>{{this._id}}</h3>
                <table class="table">
                <tr>
                    <th>Title</th>
                    <th class="th-lang-flag">EN</th>
                    <th class="th-lang-flag">HK</th>
                    <th class="th-lang-flag">JP</th>
                    <th class="th-lang-flag">KR</th>
                    <th class="th-lang-flag">FR</th>
                    <th class="th-lang-flag">DE</th>
                    <th class="th-lang-flag">ES</th>
                    <th class="th-lang-flag">RU</th>
                    <th class="th-lang-flag">VN</th>
                    <th class="th-lang-flag">SG</th>
                    <th class="th-lang-flag">MY</th>
                    <th class="th-lang-flag">ID</th>
                    <th class="th-lang-flag">TH</th>
                    <th>oprations</th>
                </tr>
                {{#each this.pages}}
                <tr>
                    <td class="td-page-title">{{this.title}}</td>
                    <td class="{{#if this.langFlag.en}}exist{{else}}none{{/if}}">{{#if this.langFlag.en}}<a href="/page/edit/{{this._id}}/en">yes</a>{{else}}<a href="/page/edit/{{this._id}}/en">+</a>{{/if}}</td>
                    <td class="{{#if this.langFlag.hk}}exist{{else}}none{{/if}}">{{#if this.langFlag.hk}}<a href="/page/edit/{{this._id}}/hk">yes</a>{{else}}<a href="/page/edit/{{this._id}}/hk">+</a>{{/if}}</td>
                    <td class="{{#if this.langFlag.jp}}exist{{else}}none{{/if}}">{{#if this.langFlag.jp}}<a href="/page/edit/{{this._id}}/jp">yes</a>{{else}}<a href="/page/edit/{{this._id}}/jp">+</a>{{/if}}</td>
                    <td class="{{#if this.langFlag.kr}}exist{{else}}none{{/if}}">{{#if this.langFlag.kr}}<a href="/page/edit/{{this._id}}/kr">yes</a>{{else}}<a href="/page/edit/{{this._id}}/kr">+</a>{{/if}}</td>
                    <td class="{{#if this.langFlag.fr}}exist{{else}}none{{/if}}">{{#if this.langFlag.fr}}<a href="/page/edit/{{this._id}}/fr">yes</a>{{else}}<a href="/page/edit/{{this._id}}/fr">+</a>{{/if}}</td>
                    <td class="{{#if this.langFlag.de}}exist{{else}}none{{/if}}">{{#if this.langFlag.de}}<a href="/page/edit/{{this._id}}/de">yes</a>{{else}}<a href="/page/edit/{{this._id}}/de">+</a>{{/if}}</td>
                    <td class="{{#if this.langFlag.es}}exist{{else}}none{{/if}}">{{#if this.langFlag.es}}<a href="/page/edit/{{this._id}}/es">yes</a>{{else}}<a href="/page/edit/{{this._id}}/es">+</a>{{/if}}</td>
                    <td class="{{#if this.langFlag.ru}}exist{{else}}none{{/if}}">{{#if this.langFlag.ru}}<a href="/page/edit/{{this._id}}/ru">yes</a>{{else}}<a href="/page/edit/{{this._id}}/ru">+</a>{{/if}}</td>
                    <td class="{{#if this.langFlag.vn}}exist{{else}}none{{/if}}">{{#if this.langFlag.vn}}<a href="/page/edit/{{this._id}}/vn">yes</a>{{else}}<a href="/page/edit/{{this._id}}/vn">+</a>{{/if}}</td>
                    <td class="{{#if this.langFlag.sg}}exist{{else}}none{{/if}}">{{#if this.langFlag.sg}}<a href="/page/edit/{{this._id}}/sg">yes</a>{{else}}<a href="/page/edit/{{this._id}}/sg">+</a>{{/if}}</td>
                    <td class="{{#if this.langFlag.my}}exist{{else}}none{{/if}}">{{#if this.langFlag.my}}<a href="/page/edit/{{this._id}}/my">yes</a>{{else}}<a href="/page/edit/{{this._id}}/my">+</a>{{/if}}</td>
                    <td class="{{#if this.langFlag.id}}exist{{else}}none{{/if}}">{{#if this.langFlag.id}}<a href="/page/edit/{{this._id}}/id">yes</a>{{else}}<a href="/page/edit/{{this._id}}/id">+</a>{{/if}}</td>
                    <td class="{{#if this.langFlag.th}}exist{{else}}none{{/if}}">{{#if this.langFlag.th}}<a href="/page/edit/{{this._id}}/th">yes</a>{{else}}<a href="/page/edit/{{this._id}}/th">+</a>{{/if}}</td>
                    <td><a href="/page/remove/{{this._id}}">remove</a></td>
                </tr>
                {{/each}}
                </table>
                {{/each}}
            </div>

            <div>
                <p><a href="/page/add">add new page</a></p>
            </div>
        </div>
    </div>
</div>